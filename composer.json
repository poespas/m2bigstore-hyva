{
    "name": "magento/project-community-edition",
    "description": "eCommerce Platform for Growth (Community Edition)",
    "type": "project",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "laminas/laminas-dependency-plugin": true,
            "magento/*": true,
            "php-http/discovery": true
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "version": "2.4.8-p3",
    "require": {
        "community-engineering/language-bg_bg": "^0.0.53",
        "community-engineering/language-cs_cz": "^0.0.9",
        "community-engineering/language-da_dk": "^0.0.59",
        "community-engineering/language-de_de": "^0.0.67",
        "community-engineering/language-el_gr": "^0.0.29",
        "community-engineering/language-en_gb": "^0.0.12",
        "community-engineering/language-en_us": "^0.0.7",
        "community-engineering/language-es_es": "^0.0.149",
        "community-engineering/language-fi_fi": "^0.0.37",
        "community-engineering/language-fr_fr": "^0.0.60",
        "community-engineering/language-hu_hu": "^0.0.21",
        "community-engineering/language-it_it": "^0.0.26",
        "community-engineering/language-nl_nl": "^0.0.67",
        "community-engineering/language-no_no": "^0.0.18",
        "community-engineering/language-pl_pl": "^0.0.42",
        "community-engineering/language-pt_br": "^0.0.79",
        "community-engineering/language-ro_ro": "^0.0.35",
        "community-engineering/language-ru_ru": "^0.0.66",
        "community-engineering/language-sk_sk": "^0.0.6",
        "community-engineering/language-sv_se": "^0.0.27",
        "community-engineering/language-tr_tr": "^0.0.28",
        "hyva-themes/magento2-default-theme": "^1.4",
        "magento/composer-dependency-version-audit-plugin": "~0.1",
        "magento/composer-root-update-plugin": "^2.0.4",
        "magento/product-community-edition": "2.4.8-p3"
    },
    "autoload": {
        "exclude-from-classmap": [
            "**/dev/**",
            "**/update/**",
            "*/*/Test/**/*Test"
        ],
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ],
        "psr-0": {
            "": [
                "app/code/",
                "generated/code/"
            ]
        },
        "psr-4": {
            "Magento\\Setup\\": "setup/src/Magento/Setup/"
        }
    },
    "require-dev": {
        "allure-framework/allure-phpunit": "^3.1",
        "carlos-mg89/oauth": "^0.8.17",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7 || ^1.0",
        "dg/bypass-finals": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.22",
        "hypernode/deploy-configuration": "^3.6",
        "magento/magento-allure-phpunit": "^3.0.2",
        "magento/magento-coding-standard": "*",
        "magento/magento2-functional-testing-framework": "^5.0",
        "pdepend/pdepend": "3.x-dev",
        "phpmd/phpmd": "3.x-dev",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^10.5",
        "symfony/finder": "^6.4"
    },
    "conflict": {
        "gene/bluefoot": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\PhpStan\\": "dev/tests/static/framework/Magento/PhpStan/",
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": {
        "private-packagist": {
            "type": "composer",
            "url": "https://hyva-themes.repo.packagist.com/localhost-loca-03ia2/"
        },
        "0": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    },
    "extra": {
        "magento-force": "override"
    }
}
